{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "trackingId": {
      "type": "string"
    },
    "orderStatus": {
      "type": "enumeration",
      "enum": [
        "Processed",
        "Shipped",
        "Delivered"
      ]
    },
    "users_permissions_role": {
      "plugin": "users-permissions",
      "model": "role"
    },
    "address": {
      "model": "address"
    },
    "order_items": {
      "via": "orderses",
      "collection": "order-item",
      "dominant": true
    }
  }
}
